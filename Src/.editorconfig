[*]
charset =  utf-8
# private consts have to be capitalized (camel case)
dotnet_naming_rule.private_consts_must_be_capitalized.symbols = private_consts
dotnet_naming_rule.private_consts_must_be_capitalized.severity = error
dotnet_naming_symbols.private_consts.required_modifiers = const, static
dotnet_naming_symbols.private_consts.applicable_accessibilities = private

dotnet_naming_rule.private_consts_must_be_capitalized.style = const_camel_cased
dotnet_naming_style.const_camel_cased.capitalization = pascal_case

# private fields have to be capitalized (camel case) and prefixed with an "_"
dotnet_naming_rule.private_fields_must_be_prefixed.symbols = private_fields
dotnet_naming_rule.private_fields_must_be_prefixed.severity = error
dotnet_naming_symbols.private_fields.applicable_kinds = field
dotnet_naming_symbols.private_fields.applicable_accessibilities = private

dotnet_naming_rule.private_fields_must_be_prefixed.style = prefixed_camel_cased
dotnet_naming_style.prefixed_camel_cased.required_prefix = _
dotnet_naming_style.prefixed_camel_cased.capitalization = pascal_case

# public members have to be capitalized (camel case)
dotnet_naming_rule.public_members_must_be_capitalized.symbols = public_symbols
dotnet_naming_rule.public_members_must_be_capitalized.severity = error
dotnet_naming_symbols.public_symbols.applicable_kinds = namespace, class, struct, interface, enum, property, method, event, delegate, local_function
dotnet_naming_symbols.public_symbols.applicable_accessibilities = public, protected, internal

dotnet_naming_rule.public_members_must_be_capitalized.style = camel_cased
dotnet_naming_style.camel_cased.capitalization = pascal_case

dotnet_sort_system_directives_first = true
dotnet_style_null_propagation = true:warning
dotnet_style_parentheses_in_other_binary_operators = always_for_clarity:warning
dotnet_style_predefined_type_for_locals_parameters_members = true:error
dotnet_style_prefer_auto_properties = true:error
dotnet_style_qualification_for_field = false:suggestion
dotnet_style_qualification_for_property = true:warning
dotnet_style_readonly_field = true:error
dotnet_style_require_accessibility_modifiers = always:error
csharp_indent_case_contents = true
csharp_indent_case_contents_when_block = true
csharp_indent_switch_labels = true
csharp_new_line_before_catch = true
csharp_new_line_before_else = true
csharp_new_line_before_finally = true
csharp_new_line_before_open_brace = all
csharp_prefer_braces = true:suggestion
csharp_space_after_colon_in_inheritance_clause = true
csharp_style_var_elsewhere = false:error
csharp_style_var_for_built_in_types = false:error
csharp_style_var_when_type_is_apparent = false:error
end_of_line = crlf
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true
